{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/nest"
  },
  "defaultProject": "rn-test-app",
  "projects": {
    "rn-test-app": {
      "root": "apps/rn-test-app",
      "sourceRoot": "apps/rn-test-app/src",
      "projectType": "application",
      "generators": {},
      "targets": {
        "start": {
          "executor": "@nrwl/react-native:start",
          "options": {
            "port": 8081
          }
        },
        "serve": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "nx start rn-test-app"
          }
        },
        "run-ios": {
          "executor": "@nrwl/react-native:run-ios",
          "options": {}
        },
        "bundle-ios": {
          "executor": "@nrwl/react-native:bundle",
          "outputs": [
            "apps/rn-test-app/build"
          ],
          "options": {
            "entryFile": "apps/rn-test-app/src/main.tsx",
            "platform": "ios",
            "bundleOutput": "dist/apps/rn-test-app/ios/main.bundle"
          }
        },
        "run-android": {
          "executor": "@nrwl/react-native:run-android",
          "options": {}
        },
        "build-android": {
          "executor": "@nrwl/react-native:build-android",
          "outputs": [
            "apps/rn-test-app/android/app/build/outputs/bundle",
            "apps/rn-test-app/android/app/build/outputs/apk"
          ],
          "options": {}
        },
        "bundle-android": {
          "executor": "@nrwl/react-native:bundle",
          "options": {
            "entryFile": "apps/rn-test-app/src/main.tsx",
            "platform": "android",
            "bundleOutput": "dist/apps/rn-test-app/android/main.bundle"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/rn-test-app/**/*.{js,ts,tsx}"
            ]
          }
        },
        "sync-deps": {
          "executor": "@nrwl/react-native:sync-deps",
          "options": {}
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/rn-test-app"
          ],
          "options": {
            "jestConfig": "apps/rn-test-app/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "inia-server": {
      "root": "apps/inia-server",
      "sourceRoot": "apps/inia-server/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/inia-server",
            "main": "apps/inia-server/src/main.ts",
            "tsConfig": "apps/inia-server/tsconfig.app.json",
            "assets": [
              "apps/inia-server/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/inia-server/src/environments/environment.ts",
                  "with": "apps/inia-server/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "inia-server:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/inia-server/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/inia-server"
          ],
          "options": {
            "jestConfig": "apps/inia-server/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "rn-expo-test-app": {
      "root": "apps/rn-expo-test-app",
      "sourceRoot": "apps/rn-expo-test-app/src",
      "projectType": "application",
      "generators": {},
      "targets": {
        "bundle": {
          "executor": "nx-react-native-expo:bundle",
          "options": {}
        },
        "start": {
          "executor": "nx-react-native-expo:start",
          "options": {}
        },
        "run-ios": {
          "executor": "nx-react-native-expo:run-ios",
          "options": {}
        },
        "run-android": {
          "executor": "nx-react-native-expo:run-android",
          "options": {}
        },
        "run-web": {
          "executor": "nx-react-native-expo:run-web",
          "options": {}
        },
        "publish": {
          "executor": "nx-react-native-expo:publish",
          "options": {}
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/rn-expo-test-app/**/*.{js,ts,tsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/rn-expo-test-app/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "gee-mobile-app": {
      "root": "apps/gee-mobile-app",
      "sourceRoot": "apps/gee-mobile-app/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "start": {
          "builder": "@nrwl/react-native:start",
          "options": {
            "port": 8081
          }
        },
        "run-ios": {
          "builder": "@nrwl/react-native:run-ios",
          "options": {}
        },
        "bundle-ios": {
          "builder": "@nrwl/react-native:bundle",
          "outputs": ["apps/gee-mobile-app/build"],
          "options": {
            "entryFile": "apps/gee-mobile-app/index.js",
            "platform": "ios",
            "bundleOutput": "dist/apps/gee-mobile-app/ios/index.bundle"
          }
        },
        "run-android": {
          "builder": "@nrwl/react-native:run-android",
          "options": {}
        },
        "build-android": {
          "builder": "@nrwl/react-native:build-android",
          "outputs": [
            "apps/gee-mobile-app/android/app/build/outputs/bundle",
            "apps/gee-mobile-app/android/app/build/outputs/apk"
          ],
          "options": {}
        },
        "bundle-android": {
          "builder": "@nrwl/react-native:bundle",
          "options": {
            "entryFile": "apps/gee-mobile-app/index.js",
            "platform": "android",
            "bundleOutput": "dist/apps/gee-mobile-app/android/index.bundle"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/gee-mobile-app/**/*.{js,ts,tsx}"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/gee-mobile-app/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}